
public class Review {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String[] breakfast = new String[3];
		breakfast[0] = "Bacon";
		breakfast[1] = "Eggs";
		breakfast[2] = "Toast";
		
		for (int i = 0; i < breakfast.length; i++) {
			System.out.println(breakfast[i]);
		}
		
		
	}

}
